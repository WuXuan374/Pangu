{
    "dataset_reader": {
        "type": "bottom_up",
        "dataset": "webq",
        "decoding_steps": 5,
        "training_option": 2
    },
    "model": {
        "type": "bottom_up",
        "EOS": "[SEP]",
        "dataset": "webq",
        "decoding_steps": 5,
        "device": 7,
        "dropout": 0.5,
        "em_augmentation": true,
        "hidden_size": 768,
        "loss_option": 1,
        "source_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "huggingface_transformer",
                    "model_name": "bert-base-uncased",
                    "pooling": true
                }
            }
        },
        "training_option": 2,
        "using_hf": true,
        "val_option": 2
    },
    "train_data_path": "data/webqsp_0107.train.json",
    "trainer": {
        "callbacks": [
            {
                "type": "track_epoch_callback"
            }
        ],
        "cuda_device": 7,
        "num_epochs": 5,
        "num_gradient_accumulation_steps": 8,
        "optimizer": {
            "type": "adam",
            "lr": 0.001,
            "parameter_groups": [
                [
                    [
                        "source_embedder"
                    ],
                    {
                        "lr": 2e-05
                    }
                ]
            ]
        },
        "validation_metric": "+EM"
    },
    "data_loader": {
        "batch_size": 1,
        "shuffle": true
    },
    "validation_data_loader": {
        "batch_size": 1,
        "shuffle": true
    },
    "validation_dataset_reader": {
        "type": "bottom_up",
        "dataset": "webq",
        "decoding_steps": 5,
        "infer": true,
        "training_option": 2
    }
}
